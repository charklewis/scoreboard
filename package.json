{
  "name": "scoreboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "lint": "eslint",
    "typecheck": "tsc",
    "db:migrate": "drizzle-kit generate:mysql --schema=./app/database/schema.ts",
    "db:push": "drizzle-kit push:mysql --config ./drizzle.config.ts"
  },
  "dependencies": {
    "@planetscale/database": "^1.11.0",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "cross-fetch": "^4.0.0",
    "drizzle-orm": "^0.28.6",
    "isbot": "^3.7.0",
    "mysql2": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^12.2.1",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/web": "^0.0.115",
    "bun-types": "^1.0.3",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.50.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18"
  }
}
